  "scripts": {
    "start": "webpack-dev-server --config webpack/config.dev.ts",
    "build": "webpack --config webpack/config.prod.ts",
    "build-info": "webpack --config webpack/config.info.ts",
    "ts": "tsc -p tsconfig.json",
    "ts-coverage": "mkdirp .coverage/typescript && typescript-coverage-report -s -t 98 -o .coverage/typescript",
    "eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "stylelint": "stylelint \"**/*.{css,scss,js,jsx,ts,tsx}\"",
    "lint": "yarn eslint && yarn stylelint",
    "codestyle": "prettier --check '**/*'",
    "format": "prettier --write '**/*'",
    "fix": "yarn eslint --fix && yarn stylelint --fix",
    "qa": "yarn lint && yarn codestyle && yarn tsc && yarn test",
    "test": "jest --no-cache",
    "test-silent": "jest --reporters=jest-silent-reporter",
    "test-coverage": "jest --coverage",
    "docz": "NODE_PATH=src docz dev",
    "docz-build": "NODE_PATH=src docz build",
    "more": "make usage",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,mdx}": "yarn eslint --fix",
    "**/*.{css,scss,html,js,jsx,ts,tsx}": "yarn stylelint --fix",
    "**/*": "yarn prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn && yarn ts && yarn test-silent && lint-staged"
    }
  },
